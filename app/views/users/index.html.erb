<div style="margin: 0px auto;width:960px;">
<bar data-toggle="modal" href="#entermodal" style="border:0px;position:fixed;bottom:0px;margin-left:400px;height:41px;">
<img src="/assets/logo.png" alt="rss feed" />
</bar>
<% if signed_in? %>
<%= link_to users_path do %><bar style="z-index:999;position:fixed;bottom:0px;right:0px;width:106px;height:40px;">
	<img src="/assets/search.png" alt="rss feed" style="opacity:0.7;margin-top:10px;width:20px;height:20px;"/>
	<h5 style="margin-top:-5px;">Search</h5>
</bar1><% end %>

<%= link_to signout_path, method: "delete" do %><bar style="z-index:999;position:fixed;bottom:0px;left:0px;width:106px;height:40px;">
	
	<h5 style="margin-top:25px;">Sign Out</h5>
</bar1><% end %>
<% end %>

</div>

<h1>Listing users</h1>

<table>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

