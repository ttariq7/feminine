



<div style="clear:both;">
<% if signed_in? %>
<div style="position:absolute;margin-right:240px;left:1%;" id="container">
  	<% if @feed_items.any? %>
	    <%= render partial: 'shared/feed_item',  collection: @feed_items %>
	<% end %>
</div>

<aside style="position:absolute;">
   <%= render 'shared/feed' %>
</aside>
<% end %>




<div style="margin: 0px auto;width:960px;">
<bar data-toggle="modal" href="#entermodal" style="border:0px;position:fixed;bottom:0px;margin-left:400px;width:160px;height:41px;">
<h2 style="text-align:center;">FEMYNYN<h2>
</bar>
</div>
<% if signed_in? %>
<section>
<div class="modal hide" id="entermodal" style="height:402px;width:402px;">
		
<square style="position:absolute;height:200px;width:200px;"></square>		
<square style="position:absolute;height:200px;width:200px;margin-left:200px;"></square>
<square style="position:absolute;height:200px;width:200px;margin-top:200px;"></square>
<square style="position:absolute;height:200px;width:200px;margin-top:200px;margin-left:200px;"></square>		
		
  <%= render 'shared/micropost_form' %>


</div>
</section>


<% else %>
<div class="modal hide" id="entermodal">
  <div class="">
    <button type="button" class="close" data-dismiss="modal">Ã—</button>
    <ul class="nav nav-tabs" style="margin-bottom:-10px;margin-top:-10px;margin-left:10px;background:transparent;">
	  <li class="active"><a href="#home" data-toggle="tab">Log In</a></li>
	  <li><a href="#profile" data-toggle="tab">Join</a></li>

	</ul>
  </div>
  <div class="">

	<div class="tab-content">
	  <div class="tab-pane active" id="home">
	  	<div class="row">
		    <%= form_for(:session, url: sessions_path) do |f| %>

		      <%= f.label :email %>
		      <%= f.text_field :email %>

		      <%= f.label :password %>
		      <%= f.password_field :password %>

		      <%= f.submit "Sign in", class: "btn btn-large btn-primary" %>
		    <% end %>

		    <p>New user? <%= link_to "Sign up now!", signup_path %></p>
		</div>
	  </div>
	  <div class="tab-pane" id="profile">
		<%= @user = User.new %>
		
		<%= form_for(@user) do |f| %>
		  <% if @user.errors.any? %>
		    <div id="error_explanation">
		      <h2><%= pluralize(@user.errors.count, "error") %> prohibited this user from being saved:</h2>

		      <ul>
		      <% @user.errors.full_messages.each do |msg| %>
		        <li><%= msg %></li>
		      <% end %>
		      </ul>
		    </div>
		  <% end %>

		  <div class="field">
		    <%= f.label :name %><br />
		    <%= f.text_field :name %>
		  </div>
		  <div class="field">
		    <%= f.label :email %><br />
		    <%= f.text_field :email %>
		  </div>
		  <div class="field">
		    <%= f.label :password %><br />
		    <%= f.password_field :password %>
		  </div>
		  <div class="field">
		    <%= f.label :password_confirmation %><br />
		    <%= f.password_field :password_confirmation %>
		  </div>
		  <div class="actions">
		    <%= f.submit %>
		  </div>
		<% end %>
		
	  </div>
	</div>
	
  </div>
</div>



<% end %>




